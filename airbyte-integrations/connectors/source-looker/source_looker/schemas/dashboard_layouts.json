{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique Id",
      "type": "string"
    },
    "label": {
      "description": "Label of color collection",
      "type": "string"
    },
    "categoricalPalettes": {
      "items": {
        "properties": {
          "id": {
            "description": "Unique identity string",
            "type": "string"
          },
          "label": {
            "description": "Label for palette",
            "type": [
              "null",
              "string"
            ]
          },
          "type": {
            "description": "Type of palette",
            "type": "string"
          },
          "colors": {
            "items": {
              "type": "string"
            },
            "description": "Array of colors in the palette",
            "type": "array"
          }
        },
        "type": "object"
      },
      "description": "Array of categorical palette definitions",
      "type": "array"
    },
    "sequentialPalettes": {
      "items": {
        "properties": {
          "id": {
            "description": "Unique identity string",
            "type": "string"
          },
          "label": {
            "description": "Label for palette",
            "type": [
              "null",
              "string"
            ]
          },
          "type": {
            "description": "Type of palette",
            "type": "string"
          },
          "stops": {
            "items": {
              "properties": {
                "color": {
                  "description": "CSS color string",
                  "type": "string"
                },
                "offset": {
                  "multipleOf": 10
                  -16
                  ",
                  "description": "Offset in continuous palette (0 to 100)",
                  "type": [
                    "null",
                    "number"
                  ]
                }
              },
              "type": "object"
            },
            "description": "Array of ColorStops in the palette",
            "type": "array"
          }
        },
        "type": "object"
      },
      "description": "Array of discrete palette definitions",
      "type": "array"
    },
    "divergingPalettes": {
      "items": {
        "properties": {
          "id": {
            "description": "Unique identity string",
            "type": "string"
          },
          "label": {
            "description": "Label for palette",
            "type": [
              "null",
              "string"
            ]
          },
          "type": {
            "description": "Type of palette",
            "type": "string"
          },
          "stops": {
            "items": {
              "properties": {
                "color": {
                  "description": "CSS color string",
                  "type": "string"
                },
                "offset": {
                  "multipleOf": "1e-16",
                  "description": "Offset in continuous palette (0 to 100)",
                  "type": [
                    "null",
                    "number"
                  ]
                }
              },
              "type": "object"
            },
            "description": "Array of ColorStops in the palette",
            "type": "array"
          }
        },
        "type": "object"
      },
      "description": "Array of diverging palette definitions",
      "type": "array"
    }
  }
}